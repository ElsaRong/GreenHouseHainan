<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/workspace_background_color"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/jack_num"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="@string/jackid"
        android:gravity="center"
        android:textColor="@color/workspace_icon_text_color"
        android:textSize="30sp" />
    
    <TextView
        android:id="@+id/start_time"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:text="@string/start_time"
        android:gravity="center"
        android:textColor="@color/workspace_icon_text_color"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/ac_two"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="@string/power_on_time"
        android:gravity="center"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/ac_one"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/power_off_time"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/ac_three"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/cycle_index"
        android:textSize="30sp" />

    <CheckBox
        android:id="@+id/check_box"
        style="@style/CustomCheckboxTheme"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="match_parent"
        android:gravity="center"
        android:clickable="false"
        android:focusable="false"
        android:focusableInTouchMode="false" />

</LinearLayout> 

